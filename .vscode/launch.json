  "configurations": [
  {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "type": "node"
  },
  {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "type": "pwa-node"
  },
      {
          "type": "pwa-node",
          "request": "launch",
          "name": "Launch Program",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${workspaceFolder}/index.ts"
      }
  ]
}